booleen est_premier(entier n){
    si(n <= 1){
        retourner Faux;
    } 
    entier i = 2;
    tantque (i * i <= n){
        si(n % i == 0) {
            retourner Faux;
        }
        sinon{
            i = i+1;
        } 
    }
    retourner Vrai;
}
entier nombre = lire();
ecrire(est_premier(nombre));




